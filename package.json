{
	"name": "mobile-console-lite",
	"version": "0.1.0",
	"description": "A console emulator for debugging on mobile and tablet",
	"main": "lib/mcl.js",
	"keywords": [
		"debug",
		"debugger",
		"log",
		"mobile",
		"tablet",
		"console.log"
	],
	"scripts": {
		"test": "yarn jest --config=./tests/jest.config.js --verbose",
		"build": "rimraf ./dist && webpack --config webpack.config.js",
		"dev": "webpack-dev-server --mode development --config webpack.dev.js",
		"demo": "npm run build && webpack-dev-server --mode production --config webpack.prod.js",
		"prepublishOnly": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Julien-Amblard/mobileConsoleLite.git"
	},
	"author": "Julien Amblard <amblard.julien@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Julien-Amblard/mobileConsoleLite/issues"
	},
	"homepage": "https://github.com/Julien-Amblard/mobileConsoleLite#readme",
	"devDependencies": {
		"@babel/cli": "^7.5.5",
		"@babel/core": "^7.5.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.5.5",
		"@babel/polyfill": "^7.6.0",
		"@babel/preset-env": "^7.6.3",
		"@babel/preset-react": "^7.6.3",
		"babel-jest": "^24.9.0",
		"babel-loader": "^8.0.6",
		"css-loader": "^3.2.0",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.15.1",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^24.9.0",
		"jest-css-modules": "^2.1.0",
		"node-sass": "^4.12.0",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"react-test-renderer": "^16.10.2",
		"rimraf": "^3.0.0",
		"sass-loader": "^8.0.0",
		"style-loader": "^1.0.0",
		"webpack": "^4.39.2",
		"webpack-cli": "^3.3.7",
		"webpack-dev-server": "^3.8.0"
	}
}
